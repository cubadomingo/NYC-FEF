    "file-loader": "^0.9.0",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.0",
    "install": "^0.8.4",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
