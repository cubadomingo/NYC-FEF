{
  "name": "NARRWB",
  "version": "1.0.0",
  "description": "Boilerplate code for a React, Redux, and Webpack project.",
  "main": "index.js",
  "scripts": {
    "test": "nodemon --watch mocha --require test/helpers/browser.js test/*.spec.js",
    "test:lint": "eslint . --cache; exit 0",
    "start": "nodemon --watch webpack.config.dev.js --exec \"webpack-dev-server --config webpack.config.dev.js --progress --inline --colors\"",
    "build": "rm -rf build && webpack --config webpack.config.prod.js --env production --hide-modules --progress"
  },
  "author": "Devin Osorio",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "react-css-modules": "^4.2.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.4",
    "react-router-dom": "4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-jsx": "^1.0.1",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "install": "^0.8.8",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.5.4",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack-dev-server": "^2.4.2"
  }
}
