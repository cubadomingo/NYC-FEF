{
  "name": "NARRWB",
  "version": "1.0.0",
  "description": "Boilerplate code for a React, Redux, and Webpack project.",
  "main": "index.js",
  "scripts": {
    "test:lint": "eslint . --cache; exit 0",
    "start": "nodemon --watch webpack.config.dev.js --exec \"webpack-dev-server --config webpack.config.dev.js --progress --inline --colors\"",
    "build": "rm -rf build && webpack --config webpack.config.prod.js --env production --hide-modules --progress"
  },
  "author": "Devin Osorio",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "4.0.0-beta.7",
    "react-router-dom": "4.0.0-beta.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.2.0-rc.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
